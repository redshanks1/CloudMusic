 发送验证码\nexport function getCode(phone){\n    return request({\n        url: '/captcha/sent',\n        params: {\n            phone \n        }\n    })\n}\n\n// 验证验证码\nexport function getCaptcha(phone,captcha){\n    return request({\n        url: '/captcha/verify',\n        params:{\n            phone,\n            captcha\n        }\n    })\n}\n\n// 查询登陆状态\nexport function loginState(){\n    return request({\n        url: '/login/status'\n    })\n}\n\n// 查询手机号是否注册\nexport function checkPhone(phone){\n    return request({\n        url: '/cellphone/existence/check',\n        params:{\n            phone\n        }\n    })\n}\n\n// 刷新登录状态\nexport function refreshLogin(){\n    return request({\n        url: '/login/refresh'\n    })\n}"]}]}