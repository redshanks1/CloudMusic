A;AAAA;AAAA;AAAA;AAAA;AAcA;;AAnBA,wDAqBA,GAAA,CAAA,IAAA,CAAA,QArBA;AAAA;;AAAA;AAqBA,qEAAA;AAAA,oBAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,IAAA,CAAA;AACA,kBAAA,SAAA,EAAA,KAAA,CAAA,SADA;AACA;AACA,kBAAA,OAAA,EAAA,KAAA,CAAA,OAFA;AAEA;AACA,kBAAA,UAAA,EAAA,KAAA,CAAA,UAHA;AAGA;AACA,kBAAA,IAAA,EAAA,KAAA,CAAA,IAJA;AAIA;AACA,kBAAA,OAAA,EAAA,KAAA,CAAA,IAAA,CAAA,SALA;AAKA;AACA,kBAAA,UAAA,EAAA,KAAA,CAAA,IAAA,CAAA,UANA;AAMA;AACA,kBAAA,QAAA,EAAA,KAAA,CAAA,IAAA,CAAA,QAPA;AAOA;AACA,kBAAA,MAAA,EAAA,KAAA,CAAA,IAAA,CAAA,MARA;AAQA;AACA,kBAAA,OAAA,EAAA,KAAA,CAAA,IAAA,CAAA,OATA,CASA;;AATA,iBAAA;AAWA;AAjCA;AAAA;AAAA;AAAA;AAAA;;AAkCA,YAAA,MAAA,CAAA,MAAA,GAlCA,CAkCA;AACA,WAnCA;AAoCA;AACA;AACA;AAlIA,GA/BA;AAmKA,EAAA,OAnKA,qBAmKA;AACA;AACA,SAAA,cAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,GAAA,EAAA,KAAA,MAAA,GAAA,GAAA;AACA;AAtKA,CAAA","sourcesContent":["<template>\n  <div class=\"infoComment\">\n    <div class=\"nav\">\n      <div class=\"left\" @click=\"back\">\n        <img src=\"~assets/img/commentList/fanhui.svg\" alt=\"\" />\n      </div>\n      <div class=\"center\">\n        {{ commentTitle }}\n      </div>\n      <div class=\"rightbox\" @click=\"fxiang\">\n        <img src=\"~assets/img/commentList/fenxiang.svg\" alt=\"\" />\n      </div>\n    </div>\n    <mscroll class=\"conscroll\" :scrollY=\"true\">\n      <div\n        class=\"content\"\n        v-if=\"commentList.length > 0\"\n        v-infinite-scroll=\"loadComment\"\n        infinite-scroll-distance=\"100\"\n        infinite-scroll-delay=\"500\"\n        infinite-scroll-immediate=\"fasle\"\n      >\n        <div class=\"item\" v-for=\"(item, index) in commentList\" :key=\"index\">\n          <div class=\"topbox\">\n            <div class=\"userImg\">\n              <img @click=\"profile(item.userId)\" v-lazy=\"item.userImg\" alt=\"\" />\n            </div>\n            <div class=\"userName\">\n              <div @click=\"profile(item.userId)\" class=\"name\">\n                {{ item.userName }}\n                <img\n                  v-if=\"item.vipType !== 0\"\n                  src=\"~assets/img/common/vip1.svg\"\n                  alt=\"\"\n                />\n              </div>\n              <div class=\"addtime\">\n                {{ item.time | getTime }}\n              </div>\n            </div>\n            <div class=\"liked\">\n              <div class=\"count\">\n                {{ item.likedCount }}\n                <img :src=\"likedImg\" alt=\"\" />\n              </div>\n            </div>\n          </div>\n          <div class=\"bottombox\">\n            <div class=\"left\"></div>\n            <div class=\"right\">\n              {{ item.content }}\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"noComment\" v-else>\n        <div class=\"text\">没有更多评论了:(</div>\n      </div>\n    </mscroll>\n    <send-comment\n      @successComment=\"successComment\"\n      :type=\"type\"\n      :id=\"$route.params.id\"\n      class=\"SendComment\"\n    ></send-comment>\n    <van-share-sheet\n      v-model=\"showShare\"\n      title=\"立即分享给好友\"\n      :options=\"options\"\n    />\n  </div>\n</template>\n\n<script>\nimport { getSheetComment } from \"network/sheetInfo\"; // 评论请求\nimport { getAlbumComment } from \"network/album\";\nimport sendComment from \"components/context/sendComment/SendComment\"; // 发送评论\nimport mscroll from \"components/common/muiScroll/MuiScroll\";\nimport mui from \"assets/mui/js/mui.min.js\"; // 引入 mui js 文件\n\nexport default {\n  name: \"SheetInfoComment\",\n  data() {\n    return {\n      commentTitle: \"\", // 评论数量\n      offset: 0, // 评论页数\n      commentList: [], // 评论内容\n      likedImg: require(\"assets/img/commentList/zan.svg\"),\n      getComMore: null,\n      showShare: false, // 分享面板\n      options: [\n        [\n          { name: \"微信\", icon: \"wechat\" },\n          { name: \"微博\", icon: \"weibo\" },\n          { name: \"QQ\", icon: \"qq\" },\n        ],\n        [\n          { name: \"复制链接\", icon: \"link\" },\n          { name: \"分享海报\", icon: \"poster\" },\n          { name: \"二维码\", icon: \"qrcode\" },\n        ],\n      ],\n      commentLength: 1, // 判断是否加载评论\n      more: true, // 是否有更多评论\n      type: 2, // 评论类型\n    };\n  },\n  components: {\n    mscroll,\n    sendComment,\n  },\n  methods: {\n    successComment(commentDetail) {\n      this.commentList.unshift({\n        content: commentDetail.content, // 评论内容\n        likedCount: 0, // 喜欢数量\n        time: commentDetail.time, // 发布时间戳\n        userImg: commentDetail.avatarUrl, // 用户头像\n        userName: commentDetail.nickname, // 用户昵称\n        userId: commentDetail.id, // 用户id\n      });\n    },\n\n    // 跳转路由\n    profile(id) {\n      this.$router.push(\"/Information/\" + id);\n      this.$store.state.isShowNav = true;\n    },\n\n    getIndex(index) {\n      this.listIndex = index;\n    },\n\n    // 分享组件\n    fxiang() {\n      this.showShare = true;\n    },\n\n    // 监听上拉加载\n    loadComment() {\n      // 发送请求\n      // 判断评论组件是否打开\n      if (this.$store.state.isShowNav === false) {\n        this.$loading.loadingShow();\n        setTimeout(() => {\n          this.getCommentList(this.$route.params.id, 100, this.offset * 100);\n          this.$loading.loadingNo();\n        }, 1000);\n      }\n    },\n\n    back() {\n      this.$emit(\"toback\");\n    },\n\n    // 封转方法\n    getCommentList(sid, list, offset) {\n      if (this.$route.params.isAlbum == \"false\") {\n        this.type = 2;  // 评论类型--歌单\n        // 发送网络请求\n        if (this.more) {\n          getSheetComment(sid, list, offset).then((res) => {\n            this.more = res.data.more;\n            this.commentTitle = \"评论(\" + res.data.total + \")\";\n            // 判断评论数量\n            this.commentLength = res.data.comments.length;\n            if (res.data.hotComments) {\n              for (const item of res.data.hotComments) {\n                this.commentList.push({\n                  commentId: item.commentId, // 评论楼层id\n                  content: item.content, // 评论内容\n                  likedCount: item.likedCount, // 喜欢数量\n                  time: item.time, // 发布时间戳\n                  userImg: item.user.avatarUrl, // 用户头像\n                  authStatus: item.user.authStatus, // 用户身份认证  1 表示歌手\n                  userName: item.user.nickname, // 用户昵称\n                  userId: item.user.userId, // 用户id\n                  vipType: item.user.vipType, // 是否开通会员\n                });\n              }\n            }\n\n            for (const itemc of res.data.comments) {\n              this.commentList.push({\n                commentId: itemc.commentId, // 评论楼层id\n                content: itemc.content, // 评论内容\n                likedCount: itemc.likedCount, // 喜欢数量\n                time: itemc.time, // 发布时间戳\n                userImg: itemc.user.avatarUrl, // 用户头像\n                authStatus: itemc.user.authStatus, // 用户身份认证  1 表示歌手\n                userName: itemc.user.nickname, // 用户昵称\n                userId: itemc.user.userId, // 用户id\n                vipType: itemc.user.vipType, // 是否开通会员\n              });\n            }\n            this.offset++; // 页数增加\n          });\n        } else {\n          this.$toast.show(\"没有更多评论了:(\", 1900);\n        }\n      } else if (this.$route.params.isAlbum == \"true\") {\n        this.type = 3;  // 评论类型--歌单\n        if (this.more) {\n          getAlbumComment(sid, list, offset).then((res) => {\n            this.more = res.data.more;\n            this.commentTitle = \"评论(\" + res.data.total + \")\";\n            // 判断评论数量\n            this.commentLength = res.data.comments.length;\n            if (res.data.hotComments) {\n              for (const item of res.data.hotComments) {\n                this.commentList.push({\n                  commentId: item.commentId, // 评论楼层id\n                  content: item.content, // 评论内容\n                  likedCount: item.likedCount, // 喜欢数量\n                  time: item.time, // 发布时间戳\n                  userImg: item.user.avatarUrl, // 用户头像\n                  authStatus: item.user.authStatus, // 用户身份认证  1 表示歌手\n                  userName: item.user.nickname, // 用户昵称\n                  userId: item.user.userId, // 用户id\n                  vipType: item.user.vipType, // 是否开通会员\n                });\n              }\n            }\n\n            for (const itemc of res.data.comments) {\n              this.commentList.push({\n                commentId: itemc.commentId, // 评论楼层id\n                content: itemc.content, // 评论内容\n                likedCount: itemc.likedCount, // 喜欢数量\n                time: itemc.time, // 发布时间戳\n                userImg: itemc.user.avatarUrl, // 用户头像\n                authStatus: itemc.user.authStatus, // 用户身份认证  1 表示歌手\n                userName: itemc.user.nickname, // 用户昵称\n                userId: itemc.user.userId, // 用户id\n                vipType: itemc.user.vipType, // 是否开通会员\n              });\n            }\n            this.offset++; // 页数增加\n          });\n        }\n      }\n    },\n  },\n  created() {\n    // 获取评论\n    this.getCommentList(this.$route.params.id, 100, this.offset * 100);\n  },\n};\n</script>\n<style scoped>\n.SendComment {\n  position: absolute;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  z-index: 20;\n}\n.infoComment {\n  width: 100%;\n  /* height: 100vh; */\n  background: #fff;\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  z-index: 60;\n}\n.CommentList {\n  width: 100%;\n  background: #fff;\n  position: absolute;\n  top: 0;\n  z-index: 60;\n}\n.nav {\n  width: 100%;\n  height: 1.198402rem;\n  display: flex;\n  color: #fff;\n  background-color: rgb(212, 81, 74);\n  box-shadow: 0 -2px 5px rgba(0, 0, 0, 0.5);\n}\n.left {\n  flex: 1;\n}\n.left img {\n  width: 0.665779rem;\n  height: 0.532623rem;\n  position: relative;\n  top: 12.5px;\n  margin-left: 6px;\n}\n.center {\n  flex: 7;\n  line-height: 1.198402rem;\n  font-size: 0.479361rem;\n  /* font-size: .399467rem; */\n}\n.rightbox {\n  flex: 1;\n}\n.rightbox img {\n  width: 0.585885rem;\n  height: 0.585885rem;\n  position: relative;\n  top: 11px;\n  margin-left: 9px;\n}\n.comment {\n  height: 13.848202rem;\n}\n.content {\n  padding: 0.426099rem 0.319574rem;\n}\n.topbox {\n  width: 100%;\n  height: 1.065246rem;\n  display: flex;\n}\n.userImg {\n  flex: 1.2;\n}\n.userName {\n  flex: 5;\n  font-size: 0.346205rem;\n  margin-top: 0.053262rem;\n}\n.name {\n  position: relative;\n}\n.name img {\n  height: 0.639148rem;\n  margin-left: 3px;\n  position: absolute;\n  top: -3px;\n}\n.liked {\n  flex: 2.2;\n}\n.addtime {\n  font-size: 0.319574rem;\n  color: #8b8b8b;\n  margin-top: -4px;\n}\n.userImg img {\n  width: 1.065246rem;\n  height: 1.065246rem;\n  border-radius: 50%;\n  overflow: hidden;\n}\n.count {\n  color: #8b8b8b;\n  margin-top: 0.213049rem;\n  font-size: 0.372836rem;\n  float: right;\n}\n.count img {\n  position: relative;\n  top: 1px;\n}\n.bottombox {\n  width: 100%;\n  display: flex;\n  padding-bottom: 0.186418rem;\n}\n.left {\n  flex: 1.2;\n}\n.right {\n  flex: 7;\n  font-size: 0.364847rem;\n  padding-bottom: 0.186418rem;\n  border-bottom: 1px solid rgb(230, 230, 230);\n}\n.conscroll {\n  top: 1.198402rem;\n  background-color: #fff;\n  height: calc(100vh - 1.065246rem - 1.065246rem);\n}\n.noComment {\n  width: 100%;\n  height: 100vh;\n  text-align: center;\n  color: #8b8b8b;\n}\n.text {\n  position: relative;\n  font-size: 0.559254rem;\n  font-weight: 550;\n  top: 45%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n</style>"],"sourceRoot":"src/components/context/sheetInfo/childrenComps"}]}