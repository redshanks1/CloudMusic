ollow} from 'network/follow'  // 网络请求\n\nexport default {\n    name: 'Follow',\n    data () {\n        return {\n            navTitle: '关注',\n            followList: [],   // 关注数据\n            uid: this.$route.params.id\n        }\n    },\n    created () {\n        // 获取关注列表\n        getUserFollow(this.$route.params.id).then(res => {\n            for (const item of res.data.follow) {\n                this.followList.push({\n                nickname: item.nickname, \n                avatarUrl: item.avatarUrl,\n                userId: item.userId,\n                signature: item.signature,\n                vipType: item.vipType,\n                followBtn: false,\n                btntext: '已关注'})\n            }\n        })\n    },\n    components: {\n        menuNav,\n        FollowItem,\n        muiScroll\n    }\n}\n\n</script>\n<style scoped>\n    .navbar{\n        position: relative;\n        z-index: 20;\n        background-color: #fff;\n        box-shadow: 0 2px 5px rgba(0, 0, 0, 0.5);\n    }\n    .Follow{\n        /* height: 100vh; */\n        background-color: #fff;\n        \n    }\n    .box{\n        padding:8px 12px;\n    }\n    .noFollow{\n        width: 100%;\n        height: 100vh;\n        text-align: center;\n        font-size: .45273rem;\n        color: rgb(133, 133, 133);\n        line-height: 85vh;\n    }\n</style>"]}]}